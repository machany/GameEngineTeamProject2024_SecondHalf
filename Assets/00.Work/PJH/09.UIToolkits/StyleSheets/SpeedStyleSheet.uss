.visual-element-speed {
    flex-grow: 0;
    background-color: rgb(121, 194, 203);
    height: 150px;
    width: 500px;
    flex-direction: row;
    justify-content: space-around;
    transition-duration: 0.25s;
}

.visual-element-data {
    flex-grow: 0;
    background-color: rgb(201, 79, 79);
    height: 200px;
    flex-shrink: 1;
    width: 200px;
}

.visual-element-speed-toggle {
    translate: 0 -200px;
    transition-duration: 0.25s;
}

.button-stop {
    width: 100px;
    height: 100px;
    background-image: url("project://database/Assets/00.Work/PJH/04.Sprites/UI/Stop.png?fileID=21300000&guid=6ddc5eb9329732548821e715b73896b6&type=3#Stop");
    background-color: rgba(255, 255, 255, 0);
    border-left-color: rgba(255, 255, 255, 0);
    border-right-color: rgba(255, 255, 255, 0);
    border-top-color: rgba(255, 255, 255, 0);
    border-bottom-color: rgba(255, 255, 255, 0);
    transition-duration: 0.25s;
}

